.swiper_view swiper {
  width: 100%;
  height: calc(45.33333333vw);
}
.swiper_view swiper navigator {
  width: 100%;
  height: 100%;
}
.swiper_view swiper navigator image {
  width: 100%;
}
.cate_view {
  display: flex;
}
.cate_view .cate_view_item {
  flex: 1;
}
.cate_view .cate_view_item navigator {
  padding: 20rpx;
}
.cate_view .cate_view_item navigator image {
  width: 100%;
}
.floor_view {
  overflow: hidden;
}
.floor_view .image_header {
  width: 100%;
  margin: 10rpx 0;
}
.floor_view navigator {
  float: left;
  width: 33.33%;
  border-left: 10rpx solid #ffffff;
}
.floor_view navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-bottom: 10rpx solid #ffffff;
}
.floor_view navigator image {
  width: 100%;
  height: 100%;
}
