/* pages/Carmen/Carmen.wxss */
page{
  height: 100%;
}
carmen{
  height: 100%;
}
.carmen .btn_d{
  width: 40%;
  padding: 20rpx;
  background-color: #e4293c;
  color: white;
  position: relative;
  z-index: 3;
}
.carmen_pz{
  position: fixed;
  width: 100%;
  height:100%;
  left: 0;
  bottom: 0;
  z-index: 2;
}
.carmen .btn_p{
  width:180rpx;
  height:180rpx;
  background-color: #e4293c;
  color: white;
  border-radius: 50%;
  line-height: 150rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  bottom: 20rpx;
  margin-left: -90rpx;
  z-index: 5;
}
.carmen .btn_q{
  width:180rpx;
  height:180rpx;
  background-color: #e4293c;
  color: white;
  border-radius: 50%;
  line-height: 150rpx;
  text-align: center;
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  z-index: 5;
 
}
.xiangC{
  width:180rpx;
  height:180rpx;
  border-radius: 50%;
  position: absolute;
  left: 20rpx;
  bottom: 20rpx;
  overflow: hidden;
  z-index: 5;
}
.xiangC video{
  width:180rpx;
  height:180rpx;
}
.overImage video{
  width: 100%;

}
