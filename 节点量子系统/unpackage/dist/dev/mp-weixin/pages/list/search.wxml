iconfont icon-fuzhi" bindtap="__e"></text></view></view><view class="profitBox"><view class="lastMongthProfit"><text>上月预估收益</text><text class="profitMoney">{{"￥"+$root.m0}}</text></view><view class="totalProfit"><text>累计预估收益</text><text class="profitMoney">{{"￥"+$root.m1}}</text></view></view></view><view class="time">{{'注册时间：'+userInfo.create_time+''}}</view></view></view><view hidden="{{!(isMark&&id=='node')}}" class="extensionBox"><text data-event-opts="{{[['tap',[['closeMark']]]]}}" class="iconfont icon-guanbi1" bindtap="__e"></text><view class="detail"><view class="detail_"><view class="station"></view><view class="detail_name"><text>{{userInfo.username}}</text><view><text class="inviteCode">{{"邀请ID："+userInfo.my_code}}</text><text data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" class="iconfont icon-fuzhi" bindtap="__e"></text></view></view><view class="profitBox"><view class="lastMongthProfit"><text>商家数</text><text class="profitMoney">{{userInfo.info?userInfo.info.business:0}}</text></view><view class="totalProfit"><text>合伙人数</text><text class="profitMoney">{{userInfo.info?userInfo.info.partner:0}}</text></view></view></view><view class="time">{{'注册时间：'+userInfo.create_time+''}}</view></view></view><view data-event-opts="{{[['tap',[['closeMark']]]]}}" hidden="{{!(isMark)}}" class="mark" bindtap="__e"></view></view>