<view class="content"><view class="contentHead" style="{{('background-color:'+headBackground+';'+'height:'+headHeight+';'+'position:'+headPosition)}}"><view class="status_bar"></view><view class="head"><view data-event-opts="{{[['tap',[['returnClick',['$event']]]]]}}" class="returnImg" bindtap="__e"><image class="returnImg_" src="/static/fanhui1.png" mode></image></view><block wx:if="{{id=='node'}}"><input class="searchInput" type="text" placeholder="请输入要查找的节点" placeholder-class="placeSearchInput" data-event-opts="{{[['input',[['__set_model',['','searchContent','$event',[]]]]]]}}" value="{{searchContent}}" bindinput="__e"/></block><block wx:if="{{id=='shop'}}"><input class="searchInput" type="text" placeholder="请输入要查找的商家" placeholder-class="placeSearchInput" data-event-opts="{{[['input',[['__set_model',['','searchContent','$event',[]]]]]]}}" value="{{searchContent}}" bindinput="__e"/></block><block wx:if="{{id=='agent'}}"><input class="searchInput" type="text" placeholder="请输入要查找的合伙人" placeholder-class="placeSearchInput" data-event-opts="{{[['input',[['__set_model',['','searchContent','$event',[]]]]]]}}" value="{{searchContent}}" bindinput="__e"/></block><text data-event-opts="{{[['tap',[['otherClick',['$event']]]]]}}" class="otherTitle" style="{{('color:'+headOtherText.color)}}" bindtap="__e">{{headOtherText.text}}</text></view></view><block wx:if="{{!isHistory}}"><view style="position:relative;top:130rpx;"><block wx:for="{{nodeList}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{id='node'}}"><block><view data-event-opts="{{[['tap',[['closeMark',[index]]]]]}}" class="list" bindtap="__e"><view class="consumerDetail"><view class="consumerDetail_"><text>{{item.username}}</text></view><text class="iconfont icon-you"></text></view></view></block></block></block><block wx:for="{{shopList}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{id=='shop'}}"><block><view data-event-opts="{{[['tap',[['closeMark',[index]]]]]}}" class="list" bindtap="__e"><image