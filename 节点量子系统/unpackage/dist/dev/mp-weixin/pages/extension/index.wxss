}
.popup text {
		color: #FFFFFF;
}
@-webkit-keyframes popup {
0% {
			opacity: 0;
}
50% {
			opacity: 1;
}
100% {
			opacity: 0;
}
}
@keyframes popup {
0% {
			opacity: 0;
}
50% {
			opacity: 1;
}
100% {
			opacity: 0;
}
}
